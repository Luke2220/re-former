<%=form_with model: @user do |form| %> 
    <%= form.label(:username, 'Username:')%>
    <%= form.text_field(:username)%><br><br>
    <%= form.label(:password, 'Password:')%>
    <%= form.text_field(:password)%><br><br>
    <%= form.label(:email, 'Email:')%>
    <%= form.text_field(:email)%><br><br>
    <%= form.submit("Submit") %>
<% end %>
